{
    "jwt_secret": {
        "doc": "JWT secret for signed token",
        "format": "String",
        "default": "yftchain",
        "env": "JWT_SECRET",
        "arg": "jwt_secret"
    },
    "jwt_secret_refresh": {
        "doc": "JWT Refresh secret for signed token",
        "format": "String",
        "default": "yftchain_refresh",
        "env": "JWT_SECRET_REFRESH",
        "arg": "jwt_secret_refresh"
    },

    "expiry_in" : {
        "doc": "expiry time for signed token",
        "format": "String",
        "default": "24h",
        "env": "JWT_EXPIRY_IN",
        "arg": "jwt_expiry_in"
    },
    "access_token_expiry_in" : {
        "doc": "expiry time for signed token",
        "format": "String",
        "default": "1h",
        "env": "ACCESS_TOKEN_EXPIRY_IN",
        "arg": "access_token_expiry_in"
    }
}
